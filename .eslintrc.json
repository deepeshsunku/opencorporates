{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2017
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": "off",
		"quotes": "off",
		"no-console": "off",
		"semi": "off",
		"no-unused-vars": "off"
	}
}